{
  "name": "graphql",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "sucrase ./src -d ./dist --transforms typescript,imports && yarn run copy-mjml",
    "start": "node dist/index.js",
    "lint": "eslint ./src --ext .ts",
    "postinstall": "prisma generate",
    "copy-mjml": "copyfiles -u 1 src/**/*.mjml dist/"
  },
  "dependencies": {
    "@graphql-yoga/plugin-disable-introspection": "^0.8.1",
    "@prisma/client": "^4.13.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/node-dijkstra": "^2.5.3",
    "@whatwg-node/server-plugin-cookies": "^0.0.6",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-and-time": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-scalars": "^1.21.3",
    "graphql-shield": "^7.6.5",
    "graphql-yoga": "^3.9.1",
    "handlebars": "^4.7.7",
    "hat": "^0.0.3",
    "jsonwebtoken": "^9.0.0",
    "mjml": "^4.14.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.5",
    "node-dijkstra": "^2.5.0",
    "nodemailer": "^6.9.1",
    "otp-generator": "^4.0.1",
    "prisma": "^4.13.0",
    "rimraf": "^3.0.2",
    "sucrase": "^3.29.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mjml": "^4.7.1",
    "@types/otp-generator": "^4.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "typescript": "^4.9.3"
  }
